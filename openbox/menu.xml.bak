<?xml version="1.0" encoding="utf-8"?>
<openbox_pipe_menu>
	<item label="File Manager">
		<action name="Execute">
			<execute>
<![CDATA[xdg-open .]]>							
			</execute>
		</action>
	</item>
	<item label="Terminal">
		<action name="Execute">
			<execute>
<![CDATA[xterm]]>							
			</execute>
		</action>
	</item>
	<item label="Web Browser">
		<action name="Execute">
			<execute>
<![CDATA[xdg-open http://]]>							
			</execute>
		</action>
	</item>
	<item label="Run command">
		<action name="Execute">
			<execute>
<![CDATA[gmrun]]>							
			</execute>
		</action>
	</item>
	<separator label="Categories"/>
	<menu id="Games" label="Games">
		<item label="Steam">
			<action name="Execute">
				<execute>
<![CDATA[/usr/bin/steam]]>									
				</execute>
			</action>
		</item>
	</menu>
	<menu id="Network" label="Network">
		<item label="Avahi SSH Server Browser">
			<action name="Execute">
				<execute>
<![CDATA[/usr/bin/bssh]]>									
				</execute>
			</action>
		</item>
		<item label="Avahi VNC Server Browser">
			<action name="Execute">
				<execute>
<![CDATA[/usr/bin/bvnc]]>									
				</execute>
			</action>
		</item>
		<item label="Chromium">
			<action name="Execute">
				<execute>
<![CDATA[chromium]]>									
				</execute>
			</action>
		</item>
		<item label="Midori Private Browsing">
			<action name="Execute">
				<execute>
<![CDATA[midori --private]]>									
				</execute>
			</action>
		</item>
		<item label="Midori">
			<action name="Execute">
				<execute>
<![CDATA[midori]]>									
				</execute>
			</action>
		</item>
		<item label="Steam">
			<action name="Execute">
				<execute>
<![CDATA[/usr/bin/steam]]>									
				</execute>
			</action>
		</item>
	</menu>
	<menu id="Settings" label="Settings">
		<item label="Customize Look and Feel">
			<action name="Execute">
				<execute>
<![CDATA[lxappearance]]>									
				</execute>
			</action>
		</item>
		<item label="NVIDIA X Server Settings">
			<action name="Execute">
				<execute>
<![CDATA[/usr/bin/nvidia-settings]]>									
				</execute>
			</action>
		</item>
		<item label="Openbox Configuration Manager">
			<action name="Execute">
				<execute>
<![CDATA[obconf]]>									
				</execute>
			</action>
		</item>
		<item label="Tint2 panel settings">
			<action name="Execute">
				<execute>
<![CDATA[tint2conf]]>									
				</execute>
			</action>
		</item>
	</menu>
	<menu id="System" label="System">
		<item label="Avahi Zeroconf Browser">
			<action name="Execute">
				<execute>
<![CDATA[/usr/bin/avahi-discover]]>									
				</execute>
			</action>
		</item>
		<item label="Htop">
			<action name="Execute">
				<execute>
<![CDATA[xterm -e 'htop']]>									
				</execute>
			</action>
		</item>
		<item label="Tint2 panel">
			<action name="Execute">
				<execute>
<![CDATA[tint2]]>									
				</execute>
			</action>
		</item>
		<item label="urxvt (client)">
			<action name="Execute">
				<execute>
<![CDATA[urxvtc]]>									
				</execute>
			</action>
		</item>
		<item label="urxvt (tabbed)">
			<action name="Execute">
				<execute>
<![CDATA[urxvt-tabbed]]>									
				</execute>
			</action>
		</item>
		<item label="urxvt">
			<action name="Execute">
				<execute>
<![CDATA[urxvt]]>									
				</execute>
			</action>
		</item>
		<item label="UXTerm">
			<action name="Execute">
				<execute>
<![CDATA[uxterm]]>									
				</execute>
			</action>
		</item>
		<item label="XTerm">
			<action name="Execute">
				<execute>
<![CDATA[xterm]]>									
				</execute>
			</action>
		</item>
	</menu>
	<separator/>
	<menu id="Advanced Settings" label="Advanced Settings">
		<item label="Conky RC">
			<action name="Execute">
				<execute>
<![CDATA[geany ~/.conkyrc]]>									
				</execute>
			</action>
		</item>
		<item label="Tint2 Panel">
			<action name="Execute">
				<execute>
<![CDATA[geany ~/.config/tint2/tint2rc]]>									
				</execute>
			</action>
		</item>
		<menu id="Obmenu-Generator" label="Obmenu-Generator">
			<item label="Menu Schema">
				<action name="Execute">
					<execute>
<![CDATA[geany ~/.config/obmenu-generator/schema.pl]]>											
					</execute>
				</action>
			</item>
			<item label="Menu Config">
				<action name="Execute">
					<execute>
<![CDATA[geany ~/.config/obmenu-generator/config.pl]]>											
					</execute>
				</action>
			</item>
			<separator/>
			<item label="Generate a pipe menu">
				<action name="Execute">
					<execute>
<![CDATA[obmenu-generator -p]]>											
					</execute>
				</action>
			</item>
			<item label="Generate a static menu">
				<action name="Execute">
					<execute>
<![CDATA[obmenu-generator -s -c]]>											
					</execute>
				</action>
			</item>
			<item label="Generate a pipe menu with icons">
				<action name="Execute">
					<execute>
<![CDATA[obmenu-generator -p -i]]>											
					</execute>
				</action>
			</item>
			<item label="Generate a static menu with icons">
				<action name="Execute">
					<execute>
<![CDATA[obmenu-generator -s -i -c]]>											
					</execute>
				</action>
			</item>
			<separator/>
			<item label="Refresh Icon Set">
				<action name="Execute">
					<execute>
<![CDATA[obmenu-generator -d]]>											
					</execute>
				</action>
			</item>
		</menu>
		<menu id="Openbox" label="Openbox">
			<item label="Reconfigure Openbox">
				<action name="Execute">
					<execute>
<![CDATA[openbox --reconfigure]]>											
					</execute>
				</action>
			</item>
			<item label="Openbox Autostart">
				<action name="Execute">
					<execute>
<![CDATA[geany ~/.config/openbox/autostart]]>											
					</execute>
				</action>
			</item>
			<item label="Openbox RC">
				<action name="Execute">
					<execute>
<![CDATA[geany ~/.config/openbox/rc.xml]]>											
					</execute>
				</action>
			</item>
			<item label="Openbox Menu">
				<action name="Execute">
					<execute>
<![CDATA[geany ~/.config/openbox/menu.xml]]>											
					</execute>
				</action>
			</item>
		</menu>
	</menu>
	<separator/>
	<item label="Lock">
		<action name="Execute">
			<execute>
<![CDATA[xscreensaver-command -lock]]>							
			</execute>
		</action>
	</item>
	<item label="Exit">
		<action name="Exit"/>
	</item>
</openbox_pipe_menu>
